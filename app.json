{
  "expo": {
    "name": "foodnav",
    "slug": "foodnav",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo/ic_launcher1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/logo/ic_launcher1.png",
      "resizeMode": "contain",
      "backgroundColor": "#8daee5",
      "height": 100,
      "width": 100
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font"]
  }
}
